{
    "manifest_version":3,
    "name":"Website Blocker",
    "version":"1.1.1",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["blocksites.js"]
        }
    ],
    "permissions":        
[ 
    "tabs","activeTab"
],
"background":{
    "service_worker":"background.js"
}
    

}